{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "SCHEDULE check_and_update",
            "type": "node",
            "request": "attach",
            "address": "localhost",
            "port": 9999,
            "localRoot": "${workspaceRoot}/dist/schedule/check_and_update/src",
            "remoteRoot": "/var/task",
            "protocol": "inspector",
            "stopOnEntry": false,
            "outFiles": ["${workspaceRoot}/dist/schedule/check_and_update/src"],
            "sourceMaps": true,
        },
        {
            "name": "Healthcheck",
            "type": "node",
            "request": "attach",
            "address": "localhost",
            "port": 9999,
            "localRoot": "${workspaceRoot}/src/public/healthcheck",
            "remoteRoot": "/var/task",
            "protocol": "inspector",
            "stopOnEntry": false,
            "sourceMaps": true,
            // "outDir": "/var/task",
        },
        {
            "name": "Get User",
            "type": "node",
            "request": "attach",
            "address": "localhost",
            "port": 9999,
            "localRoot": "${workspaceRoot}/dist/public/user/GET/src",
            "remoteRoot": "/var/task",
            "protocol": "inspector",
            "stopOnEntry": false,
            "sourceMaps": true,
            "outFiles": ["${workspaceRoot}/dist/public/user/GET/src"],
            // "outDir": "/var/task",
        }
    ]
}